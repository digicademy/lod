{namespace lod = Digicademy\Lod\ViewHelpers}<lod:RemoveEmptyLines><f:if condition="{resource}">
<f:if condition="{settings.mode} == 'embedded'">
<f:then>
<script type="application/rdf+xml">
<f:render partial="Api/Show" arguments="{_all}"/>
</script>
</f:then>
<f:else>
<f:render partial="Api/Show" arguments="{_all}"/>
</f:else>
</f:if>
</f:if>
</lod:RemoveEmptyLines>