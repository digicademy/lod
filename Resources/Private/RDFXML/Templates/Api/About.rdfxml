{namespace lod = Digicademy\Lod\ViewHelpers}<?xml version="1.0" encoding="UTF-8"?><lod:RemoveEmptyLines><f:switch expression="{action}">
<f:case value="list">
<f:render partial="Api/List" arguments="{_all}" />
</f:case>
<f:case value="show">
<f:render partial="Api/Show" arguments="{_all}" />
</f:case>
<f:defaultCase>
400 - BAD REQUEST
</f:defaultCase>
</f:switch></lod:RemoveEmptyLines>