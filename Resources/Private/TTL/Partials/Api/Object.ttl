{namespace lod = Digicademy\Lod\ViewHelpers}<f:switch expression="{lod:DetermineEntity(entity : statement.object)}">
    <f:case value="Digicademy\Lod\Domain\Model\Iri">
        {statement.predicate.namespace.prefix}:{statement.predicate.value} <{f:if(condition : '{statement.object.namespace}', then : '{statement.object.namespace.iri}', else : '')}{statement.object.value}> ;
    </f:case>
    <f:case value="Digicademy\Lod\Domain\Model\Literal">
        {statement.predicate.namespace.prefix}:{statement.predicate.value} {lod:EscapeLiteral(literal : statement.object.value, format : 'turtle')} ;
    </f:case>
    <f:case default="Digicademy\Lod\Domain\Model\Bnode">
        {statement.predicate.namespace.prefix}:{statement.predicate.value} [
        <f:for each="{lod:FetchBnode(bnode : statement.object)}" as="bnodeStatement">
            <f:render partial="Api/Object" arguments="{statement : bnodeStatement}" />
        </f:for>
        ] ;
    </f:case>
</f:switch>