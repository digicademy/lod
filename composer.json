{
    "name": "digicademy/lod",
    "type": "typo3-cms-extension",
    "description": "Linked Open Data for TYPO3",
    "homepage": "http://www.digitale-akademie.de",
    "authors": [
        {
          "name": "Torsten Schrade",
          "email": "Torsten.Schrade@adwmainz.de",
          "role": "Developer",
          "homepage": "http://www.adwmainz.de"
        }
    ],
    "license": ["GPL-2.0+"],
    "keywords": ["TYPO3", "Linked Open Data", "LOD", "RDF"],
    "support": {
        "issues": "https://github.com/digicademy/lod/issues"
    },
    "version": "1.0.0",
    "require": {
        "typo3/cms-core": ">=7.6.0 || <=9.5.99",
        "ext-json": "*"
    },
    "replace": {
        "lod": "self.version",
        "typo3-ter/lod": "self.version"
    },
    "autoload": {
        "psr-4": {
          "Digicademy\\Lod\\": "Classes/"
        }
    }
}